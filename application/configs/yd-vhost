<VirtualHost 80.237.210.48:443>
        ServerAdmin laug@yourdelivery.de
        ServerName www.yourdelivery.de:443
        DocumentRoot /home/ydadmin/htdocs/live/public/

        ErrorLog /var/log/yourdelivery/apache-error.log
        CustomLog /var/log/yourdelivery/apache-access.log combined
        ServerSignature On

        # SSL
        SSLEngine On
        SSLCertificateFile /etc/ssl/verisign/cert.crt
        SSLCertificateKeyFile /etc/ssl/verisign/private.key
        SSLCACertificateFile /etc/ssl/verisign/ca.crt

        <Directory /home/ydadmin/htdocs/live/public/>
                Options -Indexes FollowSymLinks
                AllowOverride FileInfo
        </Directory>

        Alias /phpmyadmin /var/www/phpmyadmin
        <Directory /var/www/phpmyadnin/>
            AuthType Basic
            AuthName "Yourdelivery Private Sector"
            AuthUserFile "/etc/apache2/dav_svn.passwd
            Require valid-user
        </Directory>

        Alias /info /home/ydadmin/htdocs/blog/
        <Directory /home/ydadmin/htdocs/blog/>
            Options All -Indexes
        </Directory>

        Alias /storage /home/ydadmin/htdcos/live/storage/
        <Directory /home/ydadmin/htdocs/live/storage/>
            Options All -Indexes
        </Directory>

        Alias /bit/ /home/ydadmin/htdocs/live/application/views/scripts/bit/
        Alias /viewscripts/ /home/ydadmin/htdocs/live/application/views/scripts/
        <Directory /home/ydadmin/htdocs/live/application/views/scripts/>
            Options All -Indexes
        </Directory>

        Alias /media/ /home/ydadmin/htdocs/live/public/media/
        <Directory /home/ydadmin/htdocs/live/public/media/>
            Options All -Indexes
        </Directory>
</VirtualHost>

