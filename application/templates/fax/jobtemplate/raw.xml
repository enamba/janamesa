<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE jobs SYSTEM "job.dtd">
<jobs>
    <reference></reference>
    <fax>
        <job-reference></job-reference>
        <fax-file-list>
            <fax-file>
                <file-name></file-name>
            </fax-file>
        </fax-file-list>
        <fax-options>
            <resolution>low</resolution>
            <csid>004980020207702</csid>
            <header>%0f    %d.%m.%Y %H:%M:%S    %C    %P/%T</header>
            <blacklist>no</blacklist>
            <express>yes</express>
            <schedule>now</schedule>
        </fax-options>
        <distribution-list>
            <fax-address>
                <fax-number></fax-number>
            </fax-address>
        </distribution-list>
    </fax>
</jobs>
